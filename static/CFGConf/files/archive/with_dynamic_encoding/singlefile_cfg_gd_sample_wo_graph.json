{

	"data": {
		"dotFile":"singlefile.out.dot",
		"analysisFile":"singlefile.out.json"
	},

	"layout": {
		"node": {
			"shape": "box",
			"style": "solid"
		},
		"edge": {
			"style": "solid",
			"color": "black"
		},
		"loop": {
			"background": true
		},
		"function": {
			"boundary": true
		}
	},

	"filtering": {
		"selectedNodes": [],
		"isKhop":true, 
		"K":3, 
		"minNodes":20, 
		"isLoopFilter": true, 
		"loopDepth":3,
		"isFnFilter": false
	},

	"auxRules": {
		"inDegThresh": 5,
		"isClusterOnly": true
	},

	"encodings": [
		{
			"for": "node",
			"field": "isVec",
			"type": "boolean",
			"encoding": "borderColor"
		},

		{
			"for": "node",
			"field": "fpCount",
			"type": "quantitative",
			"encoding": "backgroundColor",
			"scale": {
				"type":"linear",
				"domain": [1, 100],
				"range": ["#e5f5f9", "#2ca25f"],
				"interpolation": "hcl"
			}
		},

		{
			"for": "edge",
			"field": "weight",
			"type": "quantitative",
			"encoding": "stroke-width",
			"scale": {
				"type":"log",
				"range": [1, 10],
				"clamp": true
			}
		}

	]

}
